{
    "name": "besaah/tap-payments",
    "description": "Tap Payments Http Client",
    "keywords": [
        "http-client"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.10",
        "symfony/serializer": "~5.0",
        "symfony/property-info": "~5.0",
        "guzzlehttp/guzzle": "~6.5"
    },
    "autoload": {
        "psr-4": {
            "BeSaah\\TapPayments\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeSaah\\TapPayments\\TapPaymentsProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
